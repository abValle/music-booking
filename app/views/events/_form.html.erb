<div class="container">
<h1>Novo Evento</h1>
  <%= simple_form_for [company, event] do |f|%>
  <%= f.input :title_event %>
  <%= f.input :description_event %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.input :price %>
  <%= f.submit %>
  <% end %>
</div>
